   "source": [
    "# find factorial of n\n",
    "def factorial_n(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial_n(n-1)\n",
    "\n",
    "factorial_n(7)"
   ],
